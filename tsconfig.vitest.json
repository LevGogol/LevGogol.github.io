  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "src/**/__tests__/*"],
  "exclude": [],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.vitest.tsbuildinfo",
